all: makefileobj

.PHONY: all makefileobj

include Makefile.include

makefileobj: $(QD_TARGET_DIR)/Makefile

$(QD_TARGET_DIR)/Makefile: Makefile.obj Veritas.Formats.rfc
	mkdir -p $(QD_TARGET_DIR)
	$(QD_HOME)/qd -odir $(QD_TARGET_DIR) Veritas.Formats.rfc -types_from Veritas.Formats.Types -prefix Veritas.Formats.EverParse.
	cp $< $@
