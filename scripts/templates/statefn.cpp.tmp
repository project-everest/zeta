    , out_ { 0 }
{

}

const Record& Incr::GetRecord(int idx) const
{
    assert (idx == 0);
    return r_;
}

const Value& Incr::GetPostValue(int idx) const
{
    assert (idx == 0);
    return v_;
}

bool Incr::Touches(int idx) const
{
    assert (idx == 0);
    return true;
}


void Incr::SetOutput(ReadLog& log)
{
    out_ = log.DeserializeBigEndian<Value::value_t>();
}
